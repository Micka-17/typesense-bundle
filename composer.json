{
    "name": "micka-17/typesense-bundle",
    "description": "Un bundle Symfony pour intégrer Typesense.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mickaël",
            "email": "mickaelboi.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "symfony/framework-bundle": "^7.0",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/console": "^7.0",
        "doctrine/orm": "^3.0",
        "doctrine/doctrine-bundle": "^2.14 || ^3.0",
        "typesense/typesense-php": "^5.1",
        "symfony/http-client": "^7.3",
        "symfony/webpack-encore-bundle": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^7.0",
        "phpstan/phpstan": "^1.10",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Micka17\\TypesenseBundle\\": "src/",
            "Micka17\\TypesenseBundle\\Tests\\": "tests/",
            "Typesense\\": "vendor/typesense/typesense-php/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
